#include <iostream>
using namespace std;

class Movie{
    public:
        Movie();
        Movie(string t, double r, string g);
    private:
        string title;
        double rating;
        string genre;

};

Movie::Movie() {
    title = "";
    rating = 0;
    genre = "";
}

Movie::Movie(string t, double r, string g) {
    title = t;
    rating = r;
    genre = g;

}

int main() {
    //parsing and adding movie to the graph function
    return 0;
}
