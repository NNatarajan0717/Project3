void breadthFirstSearchHeap(Movie_Vertex* arr){
    queue<Movie_Vertex> BFSq;
    vector<Movie_Vertex> vecOfMovies;
    BFSq.push(arr[0]);
    int count = 0;

    while(!BFSq.empty()){
        for(int i = 0; i < sizeof(arr); i++){
            if(2*i+1 < BFSq.size()){
                BFSq.push(arr[2*i+1]);

            }
            if(2*i+2 < sizeof(arr)){
                BFSq.push(arr[2*i+2]);

            }

        }
    }


}
